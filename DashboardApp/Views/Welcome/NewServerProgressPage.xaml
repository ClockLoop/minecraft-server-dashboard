      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DashboardApp.Views.Welcome"
      mc:Ignorable="d" 
      d:DesignHeight="420" d:DesignWidth="380"
